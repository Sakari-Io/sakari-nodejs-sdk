    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "axios": "^0.18.0",
    "jsonwebtoken": "^8.4.0",
